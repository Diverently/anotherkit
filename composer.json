{
    "name": "diverently/anotherkit",
    "description": "Another kirby project template",
    "type": "project",
    "keywords": ["kirby", "cms", "starterkit"],
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Cordes",
            "email": "robert@diverently.com",
            "homepage": "https://diverently.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "getkirby/cms": "^3.6",
        "diverently/laravel-mix-kirby": "^1.4",
        "diesdasdigital/kirby-meta-knight": "^1.2"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "kirby-cms-path": false,
        "kirby-plugin-path": "site/plugins"
    }
}
